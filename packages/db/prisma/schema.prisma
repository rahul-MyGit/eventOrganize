generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String    @id @default(cuid())
  username        String
  email           String    @unique
  password        String
  role            UserRole
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
}


enum UserRole {
  ADMIN
  VENUE_OWNER
  ORGANIZER
  BRAND_OWNER
}